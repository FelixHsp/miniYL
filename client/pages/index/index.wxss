.swiper-wrapper{
  width: 100%;
  height: 160px;
  background: #E97658;
  position: absolute;
  top: 0;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.slide-image{
  width: 100%;
  height: 100%;
  border-radius: 8px;
}
.slide-image-wrapper{
  height: 180px;
  width: 95%;
  margin: 15px auto;
}